DROP TABLE IF EXISTS TBT_PESSOA_DEPOSITO_FATURAMENTO;
DROP TABLE IF EXISTS SYNC_PESSOA_DEPOSITO_FATURAMENTO;

CREATE TABLE IF NOT EXISTS TBT_PESSOA_DEPOSITO_FATURAMENTO ( 
    CodPessoa VARCHAR(200) NOT NULL, 
	CodDeposito VARCHAR(200) NOT NULL, 
	Deposito VARCHAR(200) NOT NULL, 
	CodLocalFaturamento VARCHAR(200) NOT NULL, 
	LocalFaturamento VARCHAR(200) NOT NULL, 
	CtrlDataOperacao  DATETIME, 
    ID NUMERIC(12, 0),
	CONSTRAINT PK_TBT_PESSOA_DEPOSITO_FATURAMENTO PRIMARY KEY (CodPessoa,CodDeposito,CodLocalFaturamento)
);


CREATE TABLE IF NOT EXISTS SYNC_PESSOA_DEPOSITO_FATURAMENTO ( 
    CodPessoa VARCHAR(200) NOT NULL, 
	CodDeposito VARCHAR(200) NOT NULL, 
	Deposito VARCHAR(200) NOT NULL, 
	CodLocalFaturamento VARCHAR(200) NOT NULL, 
	LocalFaturamento VARCHAR(200) NOT NULL, 
	CtrlDataOperacao  DATETIME, 
    ID NUMERIC(12, 0),
	CONSTRAINT PK_SYNC_PESSOA_DEPOSITO_FATURAMENTO PRIMARY KEY (CodPessoa,CodDeposito,CodLocalFaturamento)
);
